<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PVidal\Bundle\ChangelogDashboardBundle\Entity\Applicationurl" table="ApplicationURL">
    <id name="id" type="integer" column="ID">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="applicationid" type="integer" column="ApplicationID" nullable="false"/>
    <field name="environmentid" type="integer" column="EnvironmentID" nullable="false"/>
    <field name="url" type="text" column="URL" nullable="false"/>
  </entity>
</doctrine-mapping>
